{
  "name": "el-higo-restaurant",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && react-snap > /dev/null 2>&1",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "i18next": "^25.5.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.22.3",
    "react-snap": "^1.23.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "vite": "^7.1.2"
  },
  "reactSnap": {
    "source": "dist",
    "include": [
      "/",
      "/carta",
      "/el-patio",
      "/como-llegar",
      "/reservas",
      "/contacto",
      "/en",
      "/en/menu",
      "/en/the-patio",
      "/en/how-to-find-us",
      "/en/reservations",
      "/en/contact"
    ],
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-web-security",
      "--disable-features=VizDisplayCompositor"
    ],
    "waitFor": 1000,
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "crawl": true,
    "userAgent": "ReactSnap",
    "skipThirdPartyRequests": true,
    "removeBlobs": true,
    "fixWebpackChunksIssue": true
  }
}
